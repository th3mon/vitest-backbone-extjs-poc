  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "build": "npm run remove-dist && npm run copy-requirejs && tsc",
    "copy-requirejs": "mkdir -p ./dist/lib && cp node_modules/requirejs/require.js ./dist/lib/require.js",
    "remove-dist": "rm -rf dist",
    "serve": "npx http-server --port 8080 --cors",
    "watch": "npm run copy-requirejs && chokidar 'src/**/*' -c 'tsc'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "backbone": "^1.6.0",
    "jquery": "^3.7.1",
    "requirejs": "^2.3.7"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.3",
    "@types/backbone": "^1.4.22",
    "@types/jquery": "^3.5.31",
    "@types/requirejs": "^2.1.37",
    "@types/underscore": "^1.11.15",
    "@vitejs/plugin-vue": "^5.1.4",
    "chokidar": "^4.0.1",
    "chokidar-cli": "^3.0.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  }
